! ===== FIRE DEFINITIONS =====
&SURF ID='Kitchen_Fire'
   HRRPUA=800.0
   COLOR='RED'
   RAMP_Q='RAMP_Kitchen_Fire'
/

&RAMP ID='RAMP_Kitchen_Fire' T=0.000 F=0.000 /
&RAMP ID='RAMP_Kitchen_Fire' T=30.000 F=1.000 /
&RAMP ID='RAMP_Kitchen_Fire' T=120.000 F=1.000 /

&VENT XB=39.250,40.750,41.750,43.250,0.000,0.000 SURF_ID='Kitchen_Fire' /
! Fire: Kitchen_Fire

&SURF ID='Living_Fire'
   HRRPUA=400.0
   COLOR='RED'
   RAMP_Q='RAMP_Living_Fire'
/

&RAMP ID='RAMP_Living_Fire' T=10.000 F=0.000 /
&RAMP ID='RAMP_Living_Fire' T=60.000 F=1.000 /
&RAMP ID='RAMP_Living_Fire' T=300.000 F=1.000 /

&VENT XB=9.500,10.500,42.000,43.000,0.000,0.000 SURF_ID='Living_Fire' /
! Fire: Living_Fire


&DUMP DT_DEVC=5.0, DT_SLCF=10.0, NFRAMES=100 /
&DEVC XYZ=25.000,25.000,1.2, QUANTITY='TEMPERATURE', ID='TEMP_0' /
&DEVC XYZ=10.000,42.500,1.2, QUANTITY='TEMPERATURE', ID='TEMP_1' /
&DEVC XYZ=10.000,22.500,1.2, QUANTITY='TEMPERATURE', ID='TEMP_2' /
&DEVC XYZ=40.000,42.500,1.2, QUANTITY='TEMPERATURE', ID='TEMP_3' /
&DEVC XYZ=37.500,20.000,1.2, QUANTITY='TEMPERATURE', ID='TEMP_4' /
&DEVC XYZ=25.000,-5.000,1.2, QUANTITY='TEMPERATURE', ID='TEMP_5' /

!==================== SLICES (mid-height) ====================

&SLCF PBZ=2.250, QUANTITY='MASS FRACTION', SPEC_ID='SOOT' /
&SLCF PBZ=2.250, QUANTITY='TEMPERATURE' /

&ISOF QUANTITY='TEMPERATURE', VALUE=50.0 /
&ISOF QUANTITY='TEMPERATURE', VALUE=100.0 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
! &VENT MB='ZMAX', SURF_ID='OPEN' /  ! NEIN!!, this makes smoke vanish through the 'ceiling'

&TAIL /
