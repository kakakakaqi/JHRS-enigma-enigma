&HEAD CHID='floor_plan', TITLE='Dynamic Fire Simulation' /
    
&MESH IJK=52,62,3, XB=-1.000,51.000,-11.000,51.000,0.000,3.000 /

&TIME T_END=600.0 /

&REAC ID='PROPANE_REAC'
   FUEL='PROPANE'
   SOOT_YIELD=0.05
   CO_YIELD=0.01
/

&MATL ID='CONCRETE'
   CONDUCTIVITY=1.0
   SPECIFIC_HEAT=0.88
   DENSITY=2200.0
/

&SURF ID='INERT'
   MATL_ID(1,1)='CONCRETE'
   THICKNESS(1)=0.15
   COLOR='GRAY'
/

&SURF ID='FLOOR'
   COLOR='TAN'
/

&OBST XB=-1.000,51.000,-11.000,51.000,0.000,0.000, SURF_ID='FLOOR' / Main Floor
&OBST XB=20.000,30.000,0.000,0.150,0.000,3.000, SURF_ID='INERT' / Hallway South Wall
